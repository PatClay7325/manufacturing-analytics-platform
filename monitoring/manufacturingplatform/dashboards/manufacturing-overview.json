      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "${DS_POSTGRESQL}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \n  wu.name as \"Equipment\",\n  wu.equipment_type as \"Type\",\n  wu.location as \"Location\",\n  wu.status as \"Status\",\n  ROUND(CAST(pm.oee AS numeric), 1) as \"OEE %\",\n  ROUND(CAST(pm.availability AS numeric), 1) as \"Availability %\",\n  ROUND(CAST(pm.performance AS numeric), 1) as \"Performance %\",\n  ROUND(CAST(pm.quality AS numeric), 1) as \"Quality %\",\n  pm.timestamp as \"Last Update\"\nFROM work_units wu\nLEFT JOIN LATERAL (\n  SELECT * FROM performance_metrics\n  WHERE work_unit_id = wu.id\n  ORDER BY timestamp DESC\n  LIMIT 1\n) pm ON true\nWHERE wu.status != 'decommissioned'\nORDER BY pm.oee ASC NULLS LAST",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
