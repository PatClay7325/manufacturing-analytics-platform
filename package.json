  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,md,json}\"",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:push": "prisma db push",
    "prisma:seed": "tsx scripts/seed-manufacturing-minimal.ts",
    "prisma:reset": "prisma db push --force-reset && npm run prisma:seed",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:syntax": "vitest run src/__tests__/syntax/optional-chaining-validator.test.ts",
    "fix:jsx": "vitest run src/__tests__/syntax/comprehensive-jsx-fixer.test.ts",
    "fix:jsx:simple": "vitest run src/__tests__/syntax/jsx-syntax-fixer.test.ts",
    "fix:syntax": "tsx scripts/fix-optional-chaining.ts",
    "fix:syntax:dry": "tsx scripts/fix-optional-chaining.ts --dry-run",
    "fix:excessive": "tsx scripts/fix-excessive-optional-chaining.ts",
    "fix:excessive:dry": "tsx scripts/fix-excessive-optional-chaining.ts --dry-run",
    "test:errors": "vitest run --config vitest.config.error-detection.ts",
    "analyze:errors": "node scripts/fix-runtime-errors.js analyze --detailed",
    "fix:errors": "node scripts/fix-runtime-errors.js fix",
    "fix:errors:dry": "node scripts/fix-runtime-errors.js fix --dry-run",
    "generate:tests": "node scripts/generate-component-tests.js",
    "test:comprehensive": "npm run generate:tests && npm run test:errors",
    "test:unit": "vitest run",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:integration:watch": "vitest watch --config vitest.config.integration.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:variables": "jest --config=jest.config.variables.js",
    "test:variables:watch": "jest --config=jest.config.variables.js --watch",
    "test:variables:coverage": "jest --config=jest.config.variables.js --coverage",
    "test:variables:functional": "node scripts/test-variable-engine.js",
    "typecheck": "tsc --noEmit",
    "msw:init": "msw init public --no-save",
    "audit": "node simple-audit.js",
    "audit:full": "./run-audit.sh"
  },
  "prisma": {
    "seed": "tsx prisma/seed-simple.ts"
  },
