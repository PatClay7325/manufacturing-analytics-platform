    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:integration:watch": "vitest watch --config vitest.config.integration.ts",
    "test:e2e": "playwright test",
    "build:analyze": "ANALYZE=true npm run build",
    "build:production": "NODE_ENV=production npm run build",
    "lint:fix": "next lint --fix",
    "test:ci": "vitest run --coverage --reporter=verbose",
    "performance:audit": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "security:scan": "npm audit && echo 'Security scan completed'",
    "task1:validate": "vitest run src/__tests__/task1-validation.test.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:variables": "jest --config=jest.config.variables.js",
    "test:variables:watch": "jest --config=jest.config.variables.js --watch",
    "test:variables:coverage": "jest --config=jest.config.variables.js --coverage",
    "test:variables:functional": "node scripts/test-variable-engine.js",
    "typecheck": "tsc --noEmit",
    "msw:init": "msw init public --no-save",
    "audit": "node simple-audit.js",
    "audit:full": "./run-audit.sh",
    "dr:backup": "tsx src/lib/disaster-recovery/DisasterRecoveryService.ts",
