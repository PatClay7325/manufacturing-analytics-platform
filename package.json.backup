    "dr:chaos:database": "tsx src/lib/testing/RealChaosEngineering.ts database",
    "dr:setup:state-table": "tsx src/lib/disaster-recovery/infrastructure/stateTable.ts",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "NODE_ENV=production next build",
    "db:migrate": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "db:backup": "pg_dump $DATABASE_URL > backup.sql",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "performance:audit": "lighthouse http://localhost:3000 --output=json --output-path=./lighthouse-report.json",
    "security:scan": "npm audit && safety check",
    "start:production": "NODE_ENV=production next start"
